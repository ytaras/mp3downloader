<?xml version="1.0" ?>
<synth>
    <style id="backingStyle">
        <font name="Dialog" size="14"/>
        <state>
            <color value="#192a21" type="BACKGROUND"/>
            <color value="#cccccc" type="FOREGROUND"/>
        </state>
    </style>
    <bind style="backingStyle" type="region" key=".*"/>
    <style id="textfield">
        <opaque value="TRUE"/>
        <insets top="4" left="6" bottom="4" right="6"/>
        <state>
            <font name="Verdana" size="14"/>
            <color value="#ffffff" type="BACKGROUND"/>
            <color value="#000000" type="TEXT_FOREGROUND"/>
            <imagePainter method="textFieldBackground" path="/controls/text_field.jpg" sourceInsets="4 15 4 15"/>
            <insets top="4" left="15" bottom="4" right="15"/>
        </state>
    </style>
    <bind style="textfield" type="region" key="TextField"/>
    <bind style="textfield" type="region" key="PasswordField"/>

    <object id="buttonGradient" class="com.mostlymusic.downloader.gui.laf.ButtonBackgroundPainter"/>
    <style id="button">
        <painter method="buttonBackground" idref="buttonGradient"/>
        <state>
            <color value="#ffffff" type="TEXT_FOREGROUND"/>
        </state>
    </style>
    <bind style="button" key="button" type="region"/>
    <style id="table">
        <state value="ENABLED">
            <color value="#79ac00" type="TEXT_FOREGROUND"/>
            <color value="#79ac00" type="FOREGROUND"/>
            <color value="#142019" type="TEXT_BACKGROUND"/>
        </state>
        <state value="ENABLED and SELECTED">
            <color value="#142019" type="TEXT_FOREGROUND"/>
            <color value="#142019" type="FOREGROUND"/>
            <color value="#79ac00" type="TEXT_BACKGROUND"/>
        </state>
    </style>
    <bind style="table" key="table" type="region"/>
</synth>
